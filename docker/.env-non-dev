COMPOSE_PROJECT_NAME=rabbitai

# database configurations (do not modify)
DATABASE_DB=rabbitai_db
DATABASE_HOST=db
DATABASE_PASSWORD=rabbitai
DATABASE_USER=rabbitai

# database engine specific environment variables
# change the below if you prefers another database engine
DATABASE_PORT=5432
DATABASE_DIALECT=postgresql
POSTGRES_DB=rabbitaidb
POSTGRES_USER=rabbitai
POSTGRES_PASSWORD=rabbitai
#MYSQL_DATABASE=rabbitaidb
#MYSQL_USER=rabbitai
#MYSQL_PASSWORD=rabbitai
#MYSQL_RANDOM_ROOT_PASSWORD=yes

# Add the mapped in /app/pythonpath_docker which allows devs to override stuff
PYTHONPATH=/app/pythonpath:/app/docker/pythonpath_dev
REDIS_HOST=redis
REDIS_PORT=6379

FLASK_ENV=production
RABBITAI_ENV=production
RABBITAI_LOAD_EXAMPLES=yes
CYPRESS_CONFIG=false
RABBITAI_PORT=8088
